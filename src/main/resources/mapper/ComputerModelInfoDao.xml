<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xz.cmcc.dao.ComputerModelInfoDao">
	<select id="queryComputerModelInfoById" resultType="com.xz.cmcc.entity.ComputerModelInfo" flushCache="true" useCache="false">
		select pc_model_id,pc_model_name 
		from computermodelinfo 
		where pc_model_id = #{pc_model_id}
	</select>
</mapper>      